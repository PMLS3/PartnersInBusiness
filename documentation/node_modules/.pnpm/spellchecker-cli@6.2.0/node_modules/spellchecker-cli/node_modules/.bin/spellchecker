#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/spellchecker-cli@6.2.0/node_modules/spellchecker-cli/build/node_modules:/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/spellchecker-cli@6.2.0/node_modules/spellchecker-cli/node_modules:/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/spellchecker-cli@6.2.0/node_modules:/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/spellchecker-cli@6.2.0/node_modules/spellchecker-cli/build/node_modules:/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/spellchecker-cli@6.2.0/node_modules/spellchecker-cli/node_modules:/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/spellchecker-cli@6.2.0/node_modules:/Users/peetstander/Projects/o-p-e/documentation/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/index.js" "$@"
else
  exec node  "$basedir/../../build/index.js" "$@"
fi
